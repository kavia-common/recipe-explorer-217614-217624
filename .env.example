# Frontend environment variables
# IMPORTANT: Do not commit your real secrets; copy this file to .env and fill values locally.
REACT_APP_API_BASE=http://localhost:3001

# Optional frontend-configurable variables (not strictly required by this template)
REACT_APP_BACKEND_URL=http://localhost:3001
REACT_APP_FRONTEND_URL=http://localhost:3000
REACT_APP_WS_URL=
REACT_APP_NODE_ENV=development
REACT_APP_NEXT_TELEMETRY_DISABLED=1
REACT_APP_ENABLE_SOURCE_MAPS=true
REACT_APP_PORT=3000
REACT_APP_TRUST_PROXY=false
REACT_APP_LOG_LEVEL=info
REACT_APP_HEALTHCHECK_PATH=/health
REACT_APP_FEATURE_FLAGS=
REACT_APP_EXPERIMENTS_ENABLED=false
REACT_APP_SPOONACULAR_API_KEY=

# Backend (documented here for reference; set in backend container .env)
# These are not consumed by the frontend app directly.
SPOONACULAR_API_KEY=
JWT_SECRET=
JWT_ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=60
DATABASE_URL=
CORS_ALLOWED_ORIGINS=http://localhost:3000
